<OpenORB>

  <module name="tyrex" initializer="tyrex.corba.Initializer">
    <import xlink:href="${openorb.home}config/default.xml#InitRef">
       <property name="TransactionService" value="corbaname:rir:#Tyrex/TransactionFactory" />
    </import>
    
  </module>
  
  <profile name="default" xlink:href="${openorb.home}config/default.xml#default">          
     <import xlink:href="${openorb.home}config/default.xml#BOA"/>
     <import xlink:href="${openorb.home}config/default.xml#InitRef">
       <property name="NameService" value="corbaloc::1.2@localhost:2001/NameService" />
     </import>          

  </profile>  	  
  
  <profile name="ejb" extends="default">
     <import xlink:href="${openorb.home}config/rmi.xml#rmi" /> 
     <import module="tyrex"/>
  </profile>
   
</OpenORB>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 1.1//EN"
                         "http://java.sun.com/j2ee/dtds/ejb-jar_1_1.dtd">
<ejb-jar>
<enterprise-beans>
  <session>
    <description>A JNDI viewer</description>
    <ejb-name>webadmin/ViewJndi</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.clienttools.ViewJndiBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>A EJB viewer</description>
    <ejb-name>webadmin/ViewEjb</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.clienttools.ViewEjbBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>An object invoker</description>
    <ejb-name>webadmin/InvokeObject</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.clienttools.InvokeObjectBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>A Class viewer</description>
    <ejb-name>webadmin/ViewClass</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.clienttools.ViewClassBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
</enterprise-beans>
</ejb-jar>

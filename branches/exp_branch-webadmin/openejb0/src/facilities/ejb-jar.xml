<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar>
<enterprise-beans>
  <session>
    <description>The home page for the web admininstration</description>
    <ejb-name>webadmin/Home</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.HomeBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>This bean has all the odds and ends of the web administration</description>
    <ejb-name>webadmin/DefaultBean</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.DefaultHttpBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>Lists the OPENEJB_HOME/openejb.conf file</description>
    <ejb-name>webadmin/ConfigBean</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.ConfigBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>
    Lists all the system properties by calling:
    System.getProperties()
    </description>
    <ejb-name>webadmin/Properties</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.PropertiesBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>
    Lists the OPENEJB_HOME/openejb.log 
    and OPENEJB_HOME/tranaction.log files
    </description>
    <ejb-name>webadmin/ListLogs</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.ListLogsBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>An online tool for deploying EJB's</description>
    <ejb-name>webadmin/Deployment</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.DeployBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>Creates this list, and all the deployed beans</description>
    <ejb-name>webadmin/DeploymentList</ejb-name>
    <home>org.openejb.admin.web.HttpHome</home>
    <remote>org.openejb.admin.web.HttpObject</remote>
    <ejb-class>org.openejb.admin.web.DeploymentListBean</ejb-class>
    <session-type>Stateless</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
  <session>
    <description>This bean does the deployment work along with the Deployment bean</description>
    <ejb-name>deploy/webadmin/Deployer</ejb-name>
    <home>org.openejb.admin.web.DeployerHome</home>
    <remote>org.openejb.admin.web.DeployerObject</remote>
    <ejb-class>org.openejb.admin.web.DeployerBean</ejb-class>
    <session-type>Stateful</session-type>
    <transaction-type>Bean</transaction-type>
  </session>
</enterprise-beans>
</ejb-jar>